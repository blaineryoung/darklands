{"id":1,"name":"shackdeadguy","note":"","pages":[{"conditions":{"actorId":1,"actorValid":false,"itemId":1,"itemValid":false,"selfSwitchCh":"A","selfSwitchValid":false,"switch1Id":1,"switch1Valid":false,"switch2Id":1,"switch2Valid":false,"variableId":1,"variableValid":false,"variableValue":0},"directionFix":true,"image":{"tileId":0,"characterName":"Damage2","direction":4,"pattern":0,"characterIndex":0},"list":[{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["Whelp, he's dead.\\pf[1]\\n<\\pn[1]>"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["Stabbed and robbed, it looks like.\\pf[2]\\n<\\pn[2]>"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["We should be careful, whoever did this"]},{"code":401,"indent":0,"parameters":["might still be around.\\pf[3]\\n<\\pn[3]>"]},{"code":0,"indent":0,"parameters":[]}],"moveFrequency":3,"moveRoute":{"list":[{"code":0,"parameters":[]}],"repeat":true,"skippable":false,"wait":false},"moveSpeed":3,"moveType":0,"priorityType":1,"stepAnime":false,"through":false,"trigger":0,"walkAnime":true},{"conditions":{"actorId":1,"actorValid":false,"itemId":1,"itemValid":false,"selfSwitchCh":"A","selfSwitchValid":false,"switch1Id":22,"switch1Valid":true,"switch2Id":1,"switch2Valid":false,"variableId":1,"variableValid":false,"variableValue":0},"directionFix":true,"image":{"tileId":0,"characterName":"Damage2","direction":4,"pattern":0,"characterIndex":5},"list":[{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["I can't help but feel a little bad"]},{"code":401,"indent":0,"parameters":["that we let the bandit go.\\af[6]\\n<\\an[6]>"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["If it helps, I'm sure he was a terrible"]},{"code":401,"indent":0,"parameters":["person. \\af[3]\\n<\\an[3]>"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["What's this? A note?\\af[1]\\n<\\an[1]>"]},{"code":101,"indent":0,"parameters":["",0,0,1]},{"code":401,"indent":0,"parameters":["Jethro,\\n<Note>"]},{"code":401,"indent":0,"parameters":[""]},{"code":401,"indent":0,"parameters":["I just want to express my sincere gratitude for"]},{"code":401,"indent":0,"parameters":["everything you've done for the darkmoor orphanage."]},{"code":101,"indent":0,"parameters":["",0,0,1]},{"code":401,"indent":0,"parameters":["Without your decades of selfless and dedicated"]},{"code":401,"indent":0,"parameters":["service, the lives of many orphans and puppies in"]},{"code":401,"indent":0,"parameters":["Darkmoor would be far darker.\\n<Note>"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["Hmph, I'm sure they paid him a lot.\\af[3]\\n<\\an[3]>"]},{"code":101,"indent":0,"parameters":["",0,0,1]},{"code":401,"indent":0,"parameters":["I regret that we've never been able to pay you"]},{"code":401,"indent":0,"parameters":["a dime for the many hours you spend teaching and"]},{"code":401,"indent":0,"parameters":["mentoring the kids.\\n<Note>"]},{"code":101,"indent":0,"parameters":["",0,0,1]},{"code":401,"indent":0,"parameters":["I can without hesitation say that you're the best"]},{"code":401,"indent":0,"parameters":["person I've ever known.  I don't know what the kids"]},{"code":401,"indent":0,"parameters":["would do without you.\\n<Note>"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["Still think we made the right decision,"]},{"code":401,"indent":0,"parameters":["\\an[3]?\\af[6]\\n<\\an[6]>"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["Shut up. Don't you have some rites to"]},{"code":401,"indent":0,"parameters":["perform or something?\\af[3]\\n<\\an[3]>"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["Naw, he didn't die in service to my god,"]},{"code":401,"indent":0,"parameters":["so screw him.\\af[6]\\n<\\an[6]>"]},{"code":0,"indent":0,"parameters":[]}],"moveFrequency":3,"moveRoute":{"list":[{"code":0,"parameters":[]}],"repeat":true,"skippable":false,"wait":false},"moveSpeed":3,"moveType":0,"priorityType":1,"stepAnime":false,"through":false,"trigger":0,"walkAnime":true},{"conditions":{"actorId":1,"actorValid":false,"itemId":1,"itemValid":false,"selfSwitchCh":"A","selfSwitchValid":false,"switch1Id":23,"switch1Valid":true,"switch2Id":1,"switch2Valid":false,"variableId":1,"variableValid":false,"variableValue":0},"directionFix":true,"image":{"tileId":0,"characterName":"Damage2","direction":4,"pattern":0,"characterIndex":5},"list":[{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["I feel good that we at least avenged"]},{"code":401,"indent":0,"parameters":["him.  \\af[6]\\n<\\an[6]>"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["What's this? A note?\\af[1]\\n<\\an[1]>"]},{"code":101,"indent":0,"parameters":["",0,0,1]},{"code":401,"indent":0,"parameters":["Jethro,\\n<Note>"]},{"code":401,"indent":0,"parameters":[""]},{"code":401,"indent":0,"parameters":["I hope this note finds you dead.  I and the entire"]},{"code":401,"indent":0,"parameters":["province of Darkmoor are sick and tired of your many"]},{"code":101,"indent":0,"parameters":["",0,0,1]},{"code":401,"indent":0,"parameters":["crimes.  Who could forget the time you bought the"]},{"code":401,"indent":0,"parameters":["orphanage, kicked out all the kids, and turned it"]},{"code":401,"indent":0,"parameters":["into a whorehouse?  Or the great puppy purge of"]},{"code":401,"indent":0,"parameters":["34?\\n<Note>"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["I mean, it's not that bad, right?\\af[6]\\n<\\an[6]>"]},{"code":101,"indent":0,"parameters":["",0,0,1]},{"code":401,"indent":0,"parameters":["Your campaign to bankrupt the war widows was"]},{"code":401,"indent":0,"parameters":["also terrible.  Truly you're the worst person"]},{"code":401,"indent":0,"parameters":["to ever live.\\n<Note>"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["Still feel good about avenging him?\\af[3]\\n<\\an[3]>"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["Shut up.\\af[6]\\n<\\an[6]>"]},{"code":0,"indent":0,"parameters":[]}],"moveFrequency":3,"moveRoute":{"list":[{"code":0,"parameters":[]}],"repeat":true,"skippable":false,"wait":false},"moveSpeed":3,"moveType":0,"priorityType":1,"stepAnime":false,"through":false,"trigger":0,"walkAnime":true}],"x":5,"y":6},
